<?xml version="1.0"?>
<configuration>
  <configSections>
    <section name="superSocket" type="SuperSocket.SocketEngine.Configuration.SocketServiceConfig, SuperSocket.SocketEngine"/>
  </configSections>
  <appSettings>
    <add key="ServiceName" value="SupperSocketService"/>
  </appSettings>
  <superSocket  isolation="AppDomain">
    <servers>
      <server name="YourServerInstanceName" shadowCopyFiles="true" serverTypeName="YourService" ip="Any" port="9998" maxConnectionNumber="100">
      </server>
    </servers>
    <serverTypes>
      <add name="YourService" type="OilingMachinService.OilingMachinServer, OilingMachinService"/>
    </serverTypes>
  </superSocket>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2"/>
  </startup>
  <runtime>
    <gcServer enabled="true"/>
  </runtime>
</configuration>
